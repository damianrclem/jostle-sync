{
  "name": "jostle-ad-sync",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "temporal:start": "docker-compose up -d",
    "temporal:stop": "docker-compose down",
    "build": "tsc --build",
    "build.watch": "tsc --build --watch",
    "start:worker": "nodemon src/workers/worker.ts",
    "lint": "eslint .",
    "start:api": "nodemon src/api/index.ts",
    "start:mock-api:encompass": "nodemon src/mock-api/encompass/index.ts",
    "test": "jest",
    "test:unit": "jest",
    "test:int": "bash __tests__/integration/setup/run.sh --runInBand",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:run": "yarn typeorm migration:run",
    "tools:decrypt": "ts-node ./tools/decrypt"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node"
    },
    "ext": "ts",
    "watch": [
      "src"
    ]
  },
  "dependencies": {
    "axios": "^0.24.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "jsonpath": "^1.1.1",
    "pdfkit": "^0.13.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "temporalio": "^0.19.1",
    "typeorm": "^0.2.41",
    "typeorm-encrypted": "^0.6.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@jest/globals": "^27.4.6",
    "@tsconfig/node16": "^1.0.0",
    "@types/async-retry": "^1.4.3",
    "@types/body-parser-xml": "^2.0.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/handlebars-helpers": "^0.5.3",
    "@types/jest": "^27.4.0",
    "@types/jsonpath": "^0.2.0",
    "@types/mocha": "^9.0.0",
    "@types/moxios": "^0.4.14",
    "@types/node": "^16.11.21",
    "@types/pg": "^8.6.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "async-retry": "^1.3.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-deprecation": "^1.2.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-tsdoc": "^0.2.14",
    "jest": "^27.4.7",
    "moxios": "^0.4.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.5.4",
    "wait-port": "^0.2.9"
  }
}